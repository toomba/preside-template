{
    "name":"PresideCMS Toomba Skeleton",
    "version":"1.0.1",
    "author":"Toomba",
    "slug":"preside-toomba-skeleton",
    "createPackageDirectory":false,
    "homepage":"https://github.com/toomba/preside-toomba-skeleton",
    "type":"preside-skeletons",
    "location":"https://github.com/toomba/preside-toomba-skeleton/archive/1.0.1.zip",
    "repository":{
        "type":"git",
