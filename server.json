{
    "name":"Toombademo",
    "openbrowser":"false",
    "debug":false,
    "trace":false,
    "jvm":{
        "heapSize": 512,
        "args": "-Dfrlicenseservice.leasetime.hint=10"
    },
    "web":{
        "directoryBrowsing":false,
        "welcomeFiles": "index.cfm",
        "host":"0.0.0.0",
        "http":{
            "port":"80"
        },
        "rewrites":{
            "enable":true,
            "config":"${APP_DIR}/urlrewrite.xml",
            "statusPath":"/rewriteStatus",
            "configReloadSeconds":30
        }
    },
    "app":{
        "serverHomeDirectory":"/root/serverHome"
    },
    "fusionreactor":{
        "enable": "${FR_ENABLE}",
        "licenseKey": "${FR_LICENSEKEY}",
        "licenseDeactivateOnShutdown": true,
        "licenseLeaseTimeout": 10,
        "port": 8088,
        "cloudGroup": "development,toomba"
    }
}