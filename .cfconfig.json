            "storage":"true",
            "metaCacheTimeout":"60000",
            "allow":"511",
            "blob":"true",
            "host":"${LUCEE_DB_HOST}",
            "dbdriver":"MySQL",
            "password":"${LUCEE_DB_PASSWORD}",
            "database":"${LUCEE_DB_NAME}",
            "dsn":"jdbc:mysql://{host}:{port}/{database}",
            "custom":"useUnicode=true&characterEncoding=UTF-8&useSSL=false&verifyServerCertificate=false",
            "port":"3306",
            "class":"org.gjt.mm.mysql.Driver",
            "validate":"false",
            "username":"${LUCEE_DB_USERNAME}",
