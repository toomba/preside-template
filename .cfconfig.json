            "appenderArguments":{
                "streamtype":"output"
            },
            "appenderClass":"lucee.commons.io.log.log4j.appender.ConsoleAppender",
            "layoutArguments":{
                "pattern":"%25d%7Bdd%2EMM%2Eyyyy%20HH%3Amm%3Ass%2CSSS%7D%20%25%2D5p%20%5B%25c%5D%20%25m%25n"
            },
            "layoutClass":"org.apache.log4j.PatternLayout",
            "level":"ERROR"
        }
    },
    "mailServers":[
        {
            "tls":"${LUCEE_MAIL_USE_TLS}",
            "idleTimeout":"10000",
            "lifeTimeout":"60000",
            "port":"${LUCEE_MAIL_PORT}",
            "username":"${LUCEE_MAIL_USERNAME}",
            "password":"${LUCEE_MAIL_PASSWORD}",
            "ssl":"${LUCEE_MAIL_USE_SSL}",
            "smtp":"${LUCEE_MAIL_IP}"
        }
    ],
    "cacheDefaultQuery":"",
